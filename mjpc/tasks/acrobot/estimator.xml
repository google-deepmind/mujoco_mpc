<mujoco model="Acrobot Swing-Up">
  <include file="../common.xml"/>
  <include file="acrobot.xml" />

  <size memory="4K"/>

  <sensor>
    <jointpos name="shoulder" joint="shoulder"/>
    <jointpos name="elbow" joint="elbow"/>
  </sensor>

  <keyframe>
  <!-- Start from bottom position. -->
  <key name="home" qpos="3.142 0"/>
  </keyframe>
</mujoco>
