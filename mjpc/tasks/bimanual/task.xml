<mujoco model="Bimanual box manipulation">
  <include file="../common.xml"/>
  <include file="scene.xml" />

  <custom>
    <!-- agent -->
    <numeric name="agent_planner" data="0"/>
    <numeric name="agent_horizon" data="0.35"/>
    <numeric name="agent_timestep" data="0.01"/>
    <numeric name="sampling_sample_width" data="0.01"/>
    <numeric name="sampling_control_width" data="0.015"/>
    <numeric name="sampling_spline_points" data="2"/>
    <numeric name="sampling_exploration" data="0.025"/>
    <numeric name="sampling_representation" data="0"/>
  </custom>

  <sensor>
    <!-- cost -->
    <user name="Effort" dim="14" user="0 0 0 1"/>
    <user name="Box Pos." dim="3" user="0 10.0 0 25"/>
    <user name="Left Reach" dim="3" user="0 5.0e-2 0 1"/>
    <user name="Right Reach" dim="3" user="0 5.0e-2 0 1"/>
    <user name="Joint Vel." dim="14" user="0 0.0 0 1"/>
    <user name="Orientation" dim="24" user="0 10.0 0 25"/>
    <user name="Nominal" dim="14" user="0 2.5e-3 0 1.0e-1"/>

    <!-- trace -->
    <framepos name="trace0" objtype="site" objname="attachment_site_left"/>
    <framepos name="trace1" objtype="site" objname="attachment_site_right"/>

    <!-- residual -->
    <framepos name="block_pos" objtype="body" objname="block"/>
    <framepos name="goal_pos" objtype="body" objname="goal"/>
    <framepos name="ee_left" objtype="site" objname="attachment_site_left"/>
    <framepos name="ee_right" objtype="site" objname="attachment_site_right"/>
    <framepos name="block0" objtype="site" objname="block0"/>
    <framepos name="block1" objtype="site" objname="block1"/>
    <framepos name="block2" objtype="site" objname="block2"/>
    <framepos name="block3" objtype="site" objname="block3"/>
    <framepos name="block4" objtype="site" objname="block4"/>
    <framepos name="block5" objtype="site" objname="block5"/>
    <framepos name="block6" objtype="site" objname="block6"/>
    <framepos name="block7" objtype="site" objname="block7"/>
    <framepos name="goal0" objtype="site" objname="goal0"/>
    <framepos name="goal1" objtype="site" objname="goal1"/>
    <framepos name="goal2" objtype="site" objname="goal2"/>
    <framepos name="goal3" objtype="site" objname="goal3"/>
    <framepos name="goal4" objtype="site" objname="goal4"/>
    <framepos name="goal5" objtype="site" objname="goal5"/>
    <framepos name="goal6" objtype="site" objname="goal6"/>
    <framepos name="goal7" objtype="site" objname="goal7"/>
  </sensor>
</mujoco>
